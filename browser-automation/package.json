        "@types/jest": "^29.5.12",
        "@types/node": "^24.3.2",
        "@types/node-fetch": "^2.6.13",
        "express": "^4.19.2",
        "jest": "^29.7.0",
        "node-fetch": "^2.7.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
